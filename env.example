# Intelligent Web Extractor Environment Configuration
# Copy this file to .env and update with your actual values

# =============================================================================
# AI MODEL CONFIGURATION
# =============================================================================

# AI Model Type (openai, anthropic, local, hybrid)
INTELLIGENT_EXTRACTOR_MODEL_TYPE=openai

# AI Model Name
INTELLIGENT_EXTRACTOR_MODEL_NAME=gpt-4

# API Keys
INTELLIGENT_EXTRACTOR_API_KEY=your_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# API Base URLs (optional, uses defaults if not set)
OPENAI_API_BASE=https://api.openai.com/v1
ANTHROPIC_API_BASE=https://api.anthropic.com

# AI Model Parameters
INTELLIGENT_EXTRACTOR_TEMPERATURE=0.1
INTELLIGENT_EXTRACTOR_MAX_TOKENS=4000
INTELLIGENT_EXTRACTOR_TIMEOUT=60

# Embedding Model
INTELLIGENT_EXTRACTOR_EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# =============================================================================
# BROWSER CONFIGURATION
# =============================================================================

# Browser Type (chromium, firefox, webkit)
INTELLIGENT_EXTRACTOR_BROWSER_TYPE=chromium

# Browser Settings
INTELLIGENT_EXTRACTOR_HEADLESS=true
INTELLIGENT_EXTRACTOR_VIEWPORT_WIDTH=1920
INTELLIGENT_EXTRACTOR_VIEWPORT_HEIGHT=1080
INTELLIGENT_EXTRACTOR_TIMEOUT=30000

# User Agent (optional, will use rotation if not set)
INTELLIGENT_EXTRACTOR_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36

# =============================================================================
# EXTRACTION CONFIGURATION
# =============================================================================

# Default Extraction Strategy (semantic, structured, hybrid, adaptive)
INTELLIGENT_EXTRACTOR_STRATEGY=adaptive

# Confidence and Relevance Thresholds
INTELLIGENT_EXTRACTOR_CONFIDENCE_THRESHOLD=0.7
INTELLIGENT_EXTRACTOR_RELEVANCE_THRESHOLD=0.6

# Content Processing
INTELLIGENT_EXTRACTOR_MIN_CONTENT_LENGTH=100
INTELLIGENT_EXTRACTOR_MAX_CONTENT_LENGTH=100000
INTELLIGENT_EXTRACTOR_REMOVE_ADS=true
INTELLIGENT_EXTRACTOR_REMOVE_NAVIGATION=true

# =============================================================================
# PERFORMANCE CONFIGURATION
# =============================================================================

# Concurrency Settings
INTELLIGENT_EXTRACTOR_MAX_WORKERS=10
INTELLIGENT_EXTRACTOR_MAX_CONCURRENT_REQUESTS=5

# Timeouts
INTELLIGENT_EXTRACTOR_REQUEST_TIMEOUT=30
INTELLIGENT_EXTRACTOR_CONNECTION_TIMEOUT=10

# Rate Limiting
INTELLIGENT_EXTRACTOR_REQUESTS_PER_SECOND=2.0
INTELLIGENT_EXTRACTOR_REQUESTS_PER_MINUTE=60
INTELLIGENT_EXTRACTOR_DELAY_BETWEEN_REQUESTS=0.5

# Memory Management
INTELLIGENT_EXTRACTOR_MAX_MEMORY_USAGE_MB=1024
INTELLIGENT_EXTRACTOR_ENABLE_GARBAGE_COLLECTION=true

# =============================================================================
# CACHING CONFIGURATION
# =============================================================================

# Cache Settings
INTELLIGENT_EXTRACTOR_ENABLE_CACHING=true
INTELLIGENT_EXTRACTOR_CACHE_TTL_SECONDS=3600
INTELLIGENT_EXTRACTOR_CACHE_MAX_SIZE_MB=100

# =============================================================================
# LOGGING CONFIGURATION
# =============================================================================

# Log Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
INTELLIGENT_EXTRACTOR_LOG_LEVEL=INFO

# Logging Options
INTELLIGENT_EXTRACTOR_ENABLE_CONSOLE_LOGGING=true
INTELLIGENT_EXTRACTOR_ENABLE_FILE_LOGGING=false
INTELLIGENT_EXTRACTOR_LOG_PERFORMANCE_METRICS=true
INTELLIGENT_EXTRACTOR_LOG_EXTRACTION_DETAILS=false
INTELLIGENT_EXTRACTOR_LOG_AI_INTERACTIONS=false

# =============================================================================
# OUTPUT CONFIGURATION
# =============================================================================

# Output Settings
INTELLIGENT_EXTRACTOR_OUTPUT_FORMAT=json
INTELLIGENT_EXTRACTOR_INCLUDE_METADATA=true
INTELLIGENT_EXTRACTOR_INCLUDE_RAW_HTML=false
INTELLIGENT_EXTRACTOR_INCLUDE_SCREENSHOTS=false

# =============================================================================
# PROXY CONFIGURATION (OPTIONAL)
# =============================================================================

# Proxy Settings (uncomment and configure if needed)
# INTELLIGENT_EXTRACTOR_PROXY_SERVER=http://proxy.example.com:8080
# INTELLIGENT_EXTRACTOR_PROXY_USERNAME=username
# INTELLIGENT_EXTRACTOR_PROXY_PASSWORD=password

# =============================================================================
# ADVANCED CONFIGURATION
# =============================================================================

# Debug Mode
INTELLIGENT_EXTRACTOR_DEBUG_MODE=false

# Test Mode
INTELLIGENT_EXTRACTOR_TEST_MODE=false

# Dry Run Mode
INTELLIGENT_EXTRACTOR_DRY_RUN=false

# =============================================================================
# DIRECTORY CONFIGURATION
# =============================================================================

# Base Directory (optional, defaults to user home)
INTELLIGENT_EXTRACTOR_BASE_DIRECTORY=

# Output Directory (optional, defaults to ./extractions)
INTELLIGENT_EXTRACTOR_OUTPUT_DIR=./extractions

# Cache Directory (optional, defaults to ./cache)
INTELLIGENT_EXTRACTOR_CACHE_DIR=./cache

# Log Directory (optional, defaults to ./logs)
INTELLIGENT_EXTRACTOR_LOG_DIR=./logs

# =============================================================================
# MONITORING CONFIGURATION (OPTIONAL)
# =============================================================================

# Prometheus Metrics (uncomment to enable)
# INTELLIGENT_EXTRACTOR_ENABLE_METRICS=true
# INTELLIGENT_EXTRACTOR_METRICS_PORT=9090

# Redis for Caching (uncomment to enable)
# INTELLIGENT_EXTRACTOR_REDIS_URL=redis://localhost:6379
# INTELLIGENT_EXTRACTOR_REDIS_DB=0

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================

# SSL Verification (set to false for self-signed certificates)
INTELLIGENT_EXTRACTOR_VERIFY_SSL=true

# Certificate Path (optional, for custom certificates)
# INTELLIGENT_EXTRACTOR_CERT_PATH=/path/to/certificate.pem

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================

# Development Mode
INTELLIGENT_EXTRACTOR_DEV_MODE=false

# Hot Reload (for development)
INTELLIGENT_EXTRACTOR_HOT_RELOAD=false

# Profile Performance (for development)
INTELLIGENT_EXTRACTOR_PROFILE_PERFORMANCE=false 